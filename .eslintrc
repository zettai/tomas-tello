{
  "extends": ["eslint:recommended", "plugin:vue/recommended", "plugin:import/errors", "plugin:import/warnings"],
  "env": {
    "node": true
  },
  "rules": {
    "import/no-unresolved": 0,
    "import/no-unassigned-import": 0,
    "semi": ["error", "never"],
    "no-console": "off",
    "vue/require-v-for-key": "off"
  }
}
